@model GridCore<RPRxULDetalleDto>
@{
}

<div class="table-responsive table-wrapper">
	<table class="table table-bordered table-hover" id="tbULxRTRDetalle">
		<thead class="table-dark">
			<tr>
				<th style="width:5%;" class="col-header-small align-center tiny-row">Id</th>
				<th style="width:20%;" class="col-header-small align-center tiny-row">Descripción</th>
				<th style="width:5%;" class="col-header-small align-center tiny-row">Id. Prov</th>
				<th style="width:5%;" class="col-header-small align-center tiny-row">Conteo (cantidad)</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model.ListaDatos)
			{
				<tr onclick="selectListaULxRTRDetalleRow(this)">
					<td class="align-center tiny-row">@item.p_id</td>
					<td class="align-left tiny-row">@item.p_desc</td>
					<td class="align-center tiny-row">@item.p_id_prov</td>
					<td class="align-right tiny-row">@item.cantidad</td>
				</tr>
			}
		</tbody>
	</table>
</div>