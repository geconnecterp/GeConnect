@model GridCore<OtroTributoDto>
@{
    var alt = true;
    string clase = string.Empty;
}
Otros Tributos
<div class="table-responsive text-nowrap  table-wrapper">
    <table class="table table-sm mb-0 table-hover table-wrapper-fixed-head" id="tbGridOtroTributo">
        <thead class="table-dark">
            <tr class="header">
                <th style="width:5%;" class="col-header-small align-center tiny-row">Concepto</th>
                <th style="width:5%;" class="col-header-small align-center tiny-row">Base</th>
                <th style="width:5%;" class="col-header-small align-center tiny-row">Ali</th>
                <th style="width:5%;" class="col-header-small align-center tiny-row">Importe</th>
                <th style="width:0.1%;" class="col-header-small align-center tiny-row">Carro</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.ListaDatos)
            {
                if (alt) { clase = "alt"; alt = false; } else { clase = string.Empty; alt = true; }

                <tr class="@clase" onclick="selectReg(this, 'tbGridOtroTributo')">
                    <td class="text-sm-center">@item.</td>
                    <td class="text-sm-start">@item.imp</td>
                    <td class="text-sm-end">@item.base_imp</td>
                    <td class="text-sm-start">@item.alicuota</td>
                    <td class="text-sm-start">@item.importe</td>
                    <td><a class="btn btn-xs btn-outline-dark" data-interaction="@item.imp" onclick="quitarOtroTributo(this);"> <i class="bx bxs-trash"></i></a></td> @*20*@
                </tr>
            }
        </tbody>
    </table>
</div>
