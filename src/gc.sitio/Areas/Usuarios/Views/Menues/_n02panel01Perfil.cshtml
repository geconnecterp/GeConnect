@model PerfilDto

<div class="row">
    <div class="col-md-6">
        <div class="row">
            <div class="input-group input-group-sm">
                <span class="input-group-text" style="width:50px;">ID</span>
                <input type="text" asp-for="perfil_id" class="form-control inputEditable" disabled/>
                <div class="form-check form-check-inline form-switch mb-2">
                    <input class="form-check-input" type="checkbox" asp-for="Perfilactivo" disabled />
                    <label class="form-check-label" for="pBalanza">ACTIVO</label>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="input-group input-group-sm">
                <span class="input-group-text" style="width:50px;">Nombre</span>
                <input type="text" asp-for="perfil_descripcion" class="form-control inputEditable" disabled/>
                
            </div>
        </div>
       
    </div>
    <div class="col-md-6">
        <div class="row mt-1">
            <div id="divPerfilUsers"></div>
        </div>
    </div>
</div>
@section scripts{
    <script>
        perfilActual = "@Model.perfil_id";
    </script>
}