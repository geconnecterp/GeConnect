@model ProductoBarradoDto
@{
    List<ComboGenDto> lst = new List<ComboGenDto> {
        new ComboGenDto{Id="E",Descripcion="EAN13"},
        new ComboGenDto{Id="D",Descripcion="DUN14"}
};

    var cmb = new SelectList(lst,"Id","Descripcion");
}

<div class="col-md-6">
    <div class="row mt-1">
        <div id="divBarrado2"></div>
    </div>
    <div class="row mt-1" id="tab2l1">
        <div class="input-group input-group-sm">
            <span class="input-group-text">BARRADO</span>
            <input type="text" asp-for="p_id_barrado" class="form-control inputEditable" disabled/>
            <span class="input-group-text">Tipo</span>
            @Html.DropDownListFor(x=>x.tba_id,cmb,"Seleccionar...",new{@class="form-control", disabled="disabled"})

        </div>
    </div>
    <div class="row mt-1" id="tab2l2">
        <div class="input-group input-group-sm">
            <span class="input-group-text">UxP</span>
            <input type="number" id="p_unidad_pres" name="p_unidad_pres" class="form-control inputEditable barr" disabled min="0" max="9999" />
            <span class="input-group-text">UxB</span>
            <input type="number" id="p_unidad_x_bulto" name="p_unidad_x_bulto" class="form-control inputEditable barr" disabled min="0" max="9999" />
            <span class="input-group-text">BxP</span>
            <input type="number" id="p_bulto_x_piso" name="p_bulto_x_piso" class="form-control inputEditable barr" disabled min="0" max="9999" />
            <span class="input-group-text">PxP</span>
            <input type="number" id="p_piso_x_pallet" name="p_piso_x_pallet" class="form-control inputEditable barr" disabled min="0" max="9999" />
        </div>
    </div>
</div>