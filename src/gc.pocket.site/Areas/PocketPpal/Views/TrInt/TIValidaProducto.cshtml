@model ProductoBusquedaDto
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "~/areas/gestion/views/shared/_LayoutGestion.cshtml";
}
<div class="row p-2">
    <div class="d-flex d-inline-flex justify-content-start">
        <span class="mt-2">BOX:</span>
        <input type="text" class="form-control-sm" id="txtValBox" />
        <button type="button" class="btn btn-success btn-sm" id="btnValBox"><i class="bx bx-check-circle bx-sm"></i></button>
        <div class="form-check form-check-inline form-switch mt-2">
            <input class="form-check-input" type="checkbox" name="EstadoActivo" id="chkDesarma" checked disabled />
            <label class="form-check-label" for="chkDesarma">Desarma</label>
        </div>
    </div>
</div>
<div class="row p-2">
    <div class="d-flex d-inline-flex justify-content-start">
        <span class="mt-2">ID:</span>
        <input type="text" class="form-control-sm" id="txtpId" value="@Model.P_id" readonly />
        <input type="text" class="form-control-sm" value="@Model.P_desc" readonly />
        <span class="mt-2"> UP: </span>
        <input type="number" class="form-control-sm" id="txtUP" value="@Model.P_unidad_pres" />
    </div>
</div>