@model RPRAutorizacionPendienteDto
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "~/areas/gestion/views/shared/_LayoutGestion.cshtml";
    
}
<div class="container">
   <partial name="_rprCabecera" model="@Model"/>
    <div class="row">
        <div class="col-sm-6">
            <div class="input-group input-group-sm">
                <span class="input-group-text">Depósito</span>
                @Html.DropDownList("DepoId", null, "Seleccione...", new { @class = "form-control" })
            </div>            
        </div>
        <div class="col-sm-6">
            <div class="input-group input-group-sm">
                <span class="input-group-text">UL:</span>
               <input type="text" maxlength="11" id="ul_Id" class="form-control-sm" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div id="divRprGrid"></div>
        </div>
    </div>
    <div class="row">
        <div class="offset-sm-3 col-sm-6">
            <a asp-action="ResguardarAutorizacionProveedor" asp-controller="rpr" asp-area="pocketppal" asp-route-rp="@Model.Rp" class="btn btn-info btn-sm"><i class="bx bx-sm bx-arrow-back"></i><span> Volver</span></a>
            <a asp-action="CargaUL" asp-controller="rpr" asp-area="PocketPpal" id="btnContinuarRpr" class="btn btn-danger btn-sm" style="display:none;"><i class="bx bx-sm bx-right-arrow"></i><span> Confirmar</span></a>
        </div>
    </div>
</div>

@section scripts{
    <script type="text/javascript">
        var PresentarProductosSeleccionadosUrl = "@Url.Action("PresentarProductosSeleccionados", "rpr", new { area = "PocketPpal" })";

    </script>
    <script src="~/js/app/rpr.js" type="text/javascript"></script>
    <script src="~/js/app/rpr3.js" type="text/javascript"></script>

}