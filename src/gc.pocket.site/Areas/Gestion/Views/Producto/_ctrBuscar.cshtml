@model BusquedaProducto
@{
    List<ProductoListaDto> listaVacia = new();
}
<div class="card col-sm-12">
    <div class="card-body">
        <form asp-action="Busqueda" asp-controller="producto" asp-area="gestion" id="ctrBusquedaBase" method="post" class="row g-3">
            <div class="input-group">
                <span class="input-group-text">ID:</span>
                <input type="text" asp-for="Busqueda" placeholder="Ingresar ID de Producto, EAN o Id de Proveedor." class="form-control" />

               @*  <button type="button" class="btn btn-sm btn-success" id="btnBuscar">Buscar</button> *@
            </div>
        </form>
    </div>
</div>

<partial name="~/areas/gestion/views/producto/busquedaadvance.cshtml" model="@listaVacia"/>
